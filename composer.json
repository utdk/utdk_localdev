{
    "name": "utexas/utdk_localdev",
    "description": "Docksal-based local development tooling for the UT Drupal Kit.",
    "type": "utexas-development",
    "keywords": ["Drupal", "UT Drupal Kit"],
    "license": ["GPL-2.0-or-later"],
    "authors": [
      {
        "name": "Web Content Management Solutions Team, Information Technology Services, The University of Texas at Austin",
        "email": "drupal-kit-support@utlists.utexas.edu",
        "homepage": "https://drupalkit.its.utexas.edu",
        "role": "Developer"
      }
    ],
    "require": {
        "brianium/paratest": "^6.2",
        "drupal/coder": "^8.3",
        "drupal/devel": "^4.0",
        "drupal/drupal-extension": "^4.1",
        "drupal/entity_browser": "^2.0",
        "drupal/page_manager": "^4.0",
        "drupal/restui": "^1.0",
        "drush/drush": "^11 || ^12",
        "drush-ops/behat-drush-endpoint": "^9",
        "friendsoftwig/twigcs": "^6",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.1",
        "mglaman/drupal-check": "^1.1",
        "mikey179/vfsstream": "^1.6",
        "oomphinc/composer-installers-extender": "^2",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^8.4.1 || ^9",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/phpunit-bridge": "^5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
