{
    "name": "utexas/utdk_localdev",
    "description": "Docksal-based local development tooling for the UT Drupal Kit.",
    "type": "utexas-development",
    "keywords": ["Drupal", "UT Drupal Kit"],
    "license": ["GPL-2.0-or-later"],
    "authors": [
      {
        "name": "Web Content Management Solutions Team, Information Technology Services, The University of Texas at Austin",
        "email": "drupal-kit-support@utlists.utexas.edu",
        "homepage": "https://drupalkit.its.utexas.edu",
        "role": "Developer"
      }
    ],
    "require": {
        "brianium/paratest": "^7",
        "drupal/core-dev": "^10",
        "drupal/devel": "^5",
        "drupal/drupal-extension": "^5",
        "drupal/entity_browser": "^2",
        "drupal/page_manager": "^4",
        "drupal/restui": "^1",
        "drush/drush": "^11 || ^12",
        "drush-ops/behat-drush-endpoint": "^9",
        "friendsoftwig/twigcs": "^6",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3",
        "mglaman/drupal-check": "^1",
        "oomphinc/composer-installers-extender": "^2",
        "phpspec/prophecy-phpunit": "^2",
        "squizlabs/php_codesniffer": "^3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
